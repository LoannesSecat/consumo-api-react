    "react-dom": "18.2.0",
    "sass": "1.54.4",
    "vite-plugin-svgr": "2.2.1",
    "wouter": "2.9.0",
    "zustand": "4.4.6"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "2.0.1",
    "@vitest/coverage-c8": "0.24.3",
    "@vitest/ui": "0.22.0",
    "cypress": "10.6.0",
    "eslint": "7.32.0 || 8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
