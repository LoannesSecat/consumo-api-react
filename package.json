{
  "name": "movies-platform",
  "private": true,
  "version": "0.1.25",
  "description": "This website allow you to see films, series and profile of people that work or worked in the cinema world, also allow save the favorites and see it in your profile (this part of profile is in construction for now)",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "predeploy": "rd /q /s dist && yarn build && yarn preview",
    "phoenix:windows": "del /f yarn.lock && rd /s /q node_modules && yarn install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ci": "yarn install --frozen-lockfile",
    "vitest:run": "vitest",
    "vitest:ui": "vitest --ui",
    "cy:run": "cypress run",
    "cy:e2e": "cypress open --browser electron --e2e",
    "cy:component": "cypress open --browser electron --component"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "redux": "4.2.0",
    "sass": "1.54.4",
    "wouter": "2.8.0-alpha.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "2.0.1",
    "@vitest/ui": "0.22.0",
    "cypress": "10.6.0",
    "eslint": "8.23.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-react": "7.31.7",
    "happy-dom": "6.0.4",
    "redux-devtools": "3.7.0",
    "vite": "3.0.8",
    "vitest": "0.22.0"
  }
}